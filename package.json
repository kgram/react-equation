{
  "name": "react-equation",
  "version": "0.0.2",
  "description": "A react renderer for ASTs generated by equation-parser/equation-resolver",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/kgram/react-equation",
  "author": "Kristoffer Gram <kristoffer.gram@gmail.com>",
  "license": "MIT",
  "scripts": {
    "storybook": "start-storybook -p 9001 -c storybook",
    "test": "echo No testing added",
    "lint": "eslint \"src/**/*.{ts,tsx}\" rollup.config.js",
    "types": "tsc -d --emitDeclarationOnly --outDir dist",
    "bundle": "rollup -c rollup.config.js",
    "prepare": "rimraf dist && yarn lint && yarn test && yarn types && yarn bundle"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "equation-parser": "^0.1.0",
    "equation-resolver": "^0.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/react": "^5.1.9",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.7.11",
    "@types/react": "^16.8.23",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "react": "~16.8.0",
    "react-dom": "~16.8.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.26.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-multi-input": "^1.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.2"
  },
  "peerDependencies": {
    "react": "~16.8.0"
  },
  "babel": {
    "presets": [
      "@babel/typescript",
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
